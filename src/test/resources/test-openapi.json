{
  "openapi": "3.0.0",
  "info": {
    "title": "Test API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/api/v1"
    }
  ],
  "paths": {
    "/items/{itemId}": {
      "get": {
        "operationId": "getItemById",
        "summary": "Get an item by its ID",
        "parameters": [
          {
            "name": "itemId",
            "in": "path",
            "required": true,
            "schema": { "type": "string" }
          }
        ],
        "security": [
          { "ApiKeyAuth": [] }
        ]
      }
    },
    "/items": {
      "get": {
        "summary": "Get all items",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "schema": { "type": "string" }
          }
        ],
        "security": [
          { "BearerAuth": [] }
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-KEY"
      },
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}